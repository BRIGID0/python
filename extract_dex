from androguard.core.bytecodes.apk import APK
from androguard.core.bytecodes.dex import DalvikVMFormat

def extract_dex_content(dex_path):
    # Carga el archivo DEX
    with open(dex_path, 'rb') as f:
        dex_data = f.read()

    # Analiza el contenido del archivo DEX
    dex = DalvikVMFormat(dex_data)
    
    # Imprime información sobre el archivo DEX
    print(f'DEX File: {dex_path}')
    print(f'Classes: {dex.get_classes()}')
    print(f'Methods: {dex.get_methods()}')

# Ruta al archivo DEX
dex_path = 'ruta/al/archivo.dex'  # Cambia esto a la ruta de tu archivo DEX

# Llama a la función para extraer el contenido
extract_dex_content(dex_path)
